body {
  font-family: georgia; }

a {
  color: #FFFFFF; }

.info {
  font-weight: bold;
  font-size: 14px; }

.water {
  background: blue; }

.fire {
  background: red; }

.earth {
  background: green; }

.air {
  background: #aaE3F6; }

.centerMe {
  margin-left: auto;
  margin-right: auto;
  width: 400px; }

.clickable {
  text-decoration: underline;
  vertical-align: middle;
  list-style-type: none;
  display: inline; }

.clickable:hover {
  background-color: #F0F8FF;
  color: #FF0000;
  cursor: pointer; }

#newObjectives {
  padding-left: 5px;
  color: red;
  font-weight: 900; }

.notClickableYet {
  display: inline;
  text-decoration: underline;
  color: grey; }

.back {
  text-align: left;
  font-size: 16px;
  text-decoration: underline;
  cursor: pointer; }

.heading {
  width: 100%;
  text-align: center;
  font-size: 26px;
  font-family: georgia;
  font-style: italic;
  font-weight: 900;
  color: yellow;
  white-space: nowrap; }

.subHeading {
  width: 100%;
  text-align: left;
  font-size: 24px;
  font-family: georgia;
  font-style: italic;
  font-weight: 900;
  color: yellow;
  white-space: nowrap; }

.brief {
  color: white;
  width: 100%;
  text-align: center;
  font-size: 14px;
  font-family: georgia;
  font-style: italic;
  font-weight: 200; }

.chapterBorder {
  padding: 25px;
  border-style: solid;
  border-width: 5px;
  border-radius: 25px; }

.chapterHeading {
  color: gold;
  width: 100%;
  text-align: left !important;
  font-size: 32px;
  font-family: georgia;
  font-style: bold;
  font-weight: 900; }

.chapterBody {
  color: white;
  width: 100%;
  text-align: left !important;
  font-size: 14px;
  font-family: georgia;
  font-style: italic;
  font-weight: 900; }

.objectivesHeading {
  color: gold;
  width: 100%;
  text-align: left !important;
  font-size: 18px;
  font-family: georgia;
  font-style: bold;
  font-weight: 900; }

.progressHeading {
  color: green;
  width: 100%;
  text-align: left !important;
  font-size: 18px;
  font-family: georgia;
  font-style: bold;
  font-weight: 900; }

.objectivesBody {
  color: white;
  width: 100%;
  text-align: left !important;
  font-size: 14px;
  font-family: georgia;
  font-style: italic;
  font-weight: 900; }

.progressBody {
  color: white;
  width: 100%;
  text-align: left !important;
  font-size: 14px;
  font-family: georgia;
  font-style: italic;
  font-weight: 900; }

#enterPortalContainer, #enterBattlegroundContainer, #enterPracticeContainer, .continueContainer {
  display: inline-block;
  margin-left: auto;
  margin-right: auto;
  font-size: 22px;
  color: grey;
  align: center;
  text-align: center;
  border: 2px solid;
  border-radius: 25px;
  width: 300px;
  cursor: pointer;
  position: relative; }

.readyToEnterPortal, .readyToEnterBattleground, .continueContainerColor {
  background: #007700;
  color: white !important; }

.readyToEnterPortal:hover, .readyToEnterBattleground:hover, .continueContainerColor:hover {
  background: #00CC00;
  box-shadow: 2px 2px 2px 2px #999999; }

#garrisonTable {
  margin-left: auto;
  margin-right: auto; }

.garrisonRow {
  cursor: pointer; }

.garrisonRow:hover {
  box-shadow: 1px 1px 1px 1px #ffffff; }

.garrisonInfoContainer {
  position: relative; }

.garrisonInfoDiv {
  height: 100%;
  position: relative; }

.battlegroundOrderContainer {
  border-radius: 25px;
  border: 2px solid;
  position: absolute;
  bottom: 0;
  display: none; }

.selectedGarrisonRow {
  background-color: #000080; }

.objectivesMetHR, .captureHR {
  width: 50%; }

.hiddenChapterHeading {
  display: none; }

#garrisonCharacterDetails {
  color: #dd61dd;
  height: 100%; }

#garrisonTableAndCharacterDetails {
  border-color: silver;
  border-style: solid;
  border-width: 5px;
  border-radius: 25px;
  vertical-align: middle;
  width: 99%; }

#garrisonTableScroll {
  overflow: scroll;
  overflow-x: hidden;
  height: 100%; }

.garrisonCharacterDescription {
  display: inline-block;
  vertical-align: middle;
  overflow: auto;
  color: #ff8000; }

#garrisonCharacterMoreDetailsCell {
  vertical-align: top;
  text-align: left;
  color: #ff8000; }

.garrisonCharacterMoreDetailsName {
  color: yellow;
  font-size: 20px; }

#garrisonAbilitiesInfoCell {
  vertical-align: top;
  text-align: left;
  color: #ff8000; }

#garrisonCharacterImgCell {
  text-align: left; }

#garrisonCharacterAbilitiesHeading {
  text-align: center;
  font-size: 20px;
  color: yellow; }

#garrisonCharacterMoreDetailsDiv, #garrisonAbilitiesInfoDiv {
  overflow: auto;
  overflow-x: hidden; }

.abilityDescription {
  margin-left: 15px; }

.abilityName {
  font-weight: bold;
  color: #3399FF; }

#defaultGarrisonButtonContainer {
  width: 100%; }

#mainMenu {
  /* 	box-shadow: 10px 10px 5px #888888; */
  border-style: solid;
  border-bottom-style: groove;
  border-radius: 5px; }

#difficultyContainer, #garrisonSortOrderContainer, #displaySettingsContainer {
  text-align: left;
  padding-left: 20px; }

#difficultyRadioContainer, #garrisonSortOrderRadioContainer, #displaySettingsRadioContainer {
  padding-left: 20px; }

#enterPortalCanvas {
  position: absolute;
  width: 100%;
  height: 100%;
  left: 0px; }

#loadingMessage {
  text-align: center;
  font-size: 28px;
  color: yellow; }

#loadingProgress {
  text-align: center;
  font-size: 20px;
  color: green; }

#gameVersion {
  width: 100%;
  text-align: center;
  font-size: 12px;
  font-family: georgia;
  font-style: italic;
  font-weight: 900;
  color: yellow;
  white-space: nowrap; }
